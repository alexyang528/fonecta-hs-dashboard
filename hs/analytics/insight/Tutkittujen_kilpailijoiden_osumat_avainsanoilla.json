{
  "$id": "hs_Tutkittujen_kilpailijoiden_osumat_avainsanoilla",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Tutkittujen kilpailijoiden osumat avainsanoilla",
  "query": {
    "metrics": [
      "yext/TOTAL_MATCHES"
    ],
    "dimensions": [
      "yext/LRT_SEARCH_RESULT_TYPE",
      "yext/LRT_COMPETITOR",
      "yext/LRT_KEYWORD"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_13"
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/TOTAL_MATCHES"
        },
        "descending": true
      }
    }
  }
}