{
  "$id": "tutkittujen_kilpailijoiden_osumatyypit_Google-mainosten_osumat",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Tutkittujen kilpailijoiden osumatyypit & Google-mainosten osumat",
  "query": {
    "metrics": [
      "yext/TOTAL_MATCHES",
      "yext/AD_MATCHES"
    ],
    "dimensions": [
      "yext/LRT_COMPETITOR",
      "yext/LRT_SEARCH_RESULT_TYPE"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_13"
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "dimension": "yext/LRT_COMPETITOR"
        }
      }
    }
  }
}