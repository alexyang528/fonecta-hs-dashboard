{
  "$id": "hs_Orgaanisten_osumien_kehitys_avainsanoittain",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Google-yritysprofiilin osumien kehitys avainsanoittain",
  "query": {
    "metrics": [
      "yext/TOTAL_MATCHES"
    ],
    "dimensions": [
      "yext/LRT_KEYWORD"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_13"
      },
      {
        "filter": "yext/matchType",
        "in": [
          "NO_MATCH",
          "LOCAL_PACK"
        ]
      },
      {
        "filter": "yext/searchEngine",
        "in": [
          "GOOGLE_DESKTOP",
          "GOOGLE_MOBILE"
        ]
      }
    ],
    "comparison": "PREVIOUS_PERIOD"
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/TOTAL_MATCHES"
        },
        "descending": true
      }
    }
  }
}