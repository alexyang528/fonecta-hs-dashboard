{
  "$id": "hs_Alykkaan_haun_osuus_avainsanoittain",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Älykkään haun osuus avainsanoittain",
  "query": {
    "metrics": [
      "yext/SHARE_OF_INTELLIGENT_SEARCH"
    ],
    "dimensions": [
      "yext/LRT_KEYWORD"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_13"
      }
    ],
    "comparison": "PREVIOUS_PERIOD"
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/SHARE_OF_INTELLIGENT_SEARCH"
        },
        "descending": true
      }
    }
  }
}