{
  "$id": "hs_Orgaaniset_osumat_avainsanoittain",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Orgaaniset osumat avainsanoittain",
  "query": {
    "metrics": [
      "yext/TOTAL_MATCHES"
    ],
    "dimensions": [
      "yext/LRT_KEYWORD"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "MONTHS_13"
      },
      {
        "filter": "yext/matchType",
        "in": [
          "NO_MATCH",
          "LISTINGS",
          "LOCATION_PAGES",
          "CORPORATE_WEBSITE"
        ]
      },
      {
        "filter": "yext/searchEngine",
        "in": [
          "BING_DESKTOP",
          "GOOGLE_DESKTOP",
          "GOOGLE_MOBILE"
        ]
      }
    ]
  },
  "display": {
    "bar": {
      "horizontal": true
    }
  }
}